{
  "name": "ganomede-events",
  "version": "1.3.0",
  "api": "events/v1",
  "description": "NodeJS pub/sub library using ganomede-notifications",
  "main": "index.ts",
  "scripts": {
    "prepublish": "npm run snyk-protect && npm run lint && npm run test",
    "start": "node -r source-map-support/register build/src/index.js",
    "startw": "nodemon --exec \"npm start\" --watch build",
    "test": "API_SECRET=1 mocha --colors --require ts-node/register --extensions ts tests/*.test.ts tests/**/*.test.ts | bunyan -l error -o short --color",
    "testw": "nodemon --watch src/ --watch tests/ --watch config.js --exec \"npm test\"",
    "coverage": "LOG_LEVEL=FATAL API_SECRET=12345678 istanbul cover _mocha -- -b tests/helper.js tests/**/*.test.js && istanbul report",
    "lint": "eslint src/ tests/ index.js config.js",
    "snyk-protect": "snyk protect",
    "trans": "tsc -b -v -i -w",
    "build": "tsc",
    "buildw": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j3k0/ganomede-events.git"
  },
  "keywords": [
    "ganomede",
    "pubsub",
    "events"
  ],
  "author": "Fovea <contact@fovea.cc>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j3k0/ganomede-events/issues"
  },
  "homepage": "https://github.com/j3k0/ganomede-events#readme",
  "devDependencies": {
    "@types/async": "^2.4.2",
    "@types/bunyan": "^1.8.7",
    "@types/chai": "^3.5.2",
    "@types/eslint": "^7.28.2",
    "@types/istanbul": "^0.4.30",
    "@types/lodash": "^4.14.176",
    "@types/mocha": "^9.0.0",
    "@types/node-statsd": "^0.1.3",
    "@types/nodemon": "^1.19.1",
    "@types/redis": "^2.8.32",
    "@types/restify": "^4.3.8",
    "@types/supertest": "^2.0.11",
    "chai": "^3.5.0",
    "eslint": "^3.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^9.1.3",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0",
    "testdouble": "^1.9.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "async": "^2.1.2",
    "bunyan": "^1.8.1",
    "curtain-down": "^1.0.0",
    "ganomede-base-client": "^1.1.0",
    "lodash": "^4.17.2",
    "node-statsd": "^0.1.1",
    "redis": "^2.6.5",
    "restify": "^4.3.1",
    "snyk": "^1.30.1"
  },
  "snyk": true
}
